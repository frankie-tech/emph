{
	"name": "rockbell",
	"version": "0.1.0",
	"description": "Collection of snippets that are useful in day to day development.",
	"license": "MIT",
	"author": {
		"name": "frankie",
		"url": "https://frankie.tech"
	},
	"source": "src/rockbell.ts",
	"main": "dist/rockbell.js",
	"unpkg": "dist/rockbell.umd.js",
	"scripts": {
		"clean": "rm -rf dist",
		"microbundle:build": "microbundle -f modern,umd --no-sourcemap --raw=true",
		"microbundle:start": "microbundle watch --compress false -f modern,umd --no-sourcemap --raw=true",
		"build": "npm-run-all clean microbundle:build",
		"start": "npm-run-all clean microbundle:start"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/frankie-tech/rockbell"
	},
	"browserslist": [
		"last 2 versions",
		"> 0.25%"
	],
	"devDependencies": {
		"microbundle": "^0.12.0",
		"npm-run-all": "^4.1.5"
	}
}
